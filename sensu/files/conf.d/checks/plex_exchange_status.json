{
  "checks": {
    "plex_exchange_status": {
      "command": "check-http.rb --timeout 40 --response-bytes 500 --response-code 200 -k --url http://localhost:8080/exchangestatus/",
      "subscribers": [
        "plex"
      ],
      "handlers": [
        "pager_team"
      ],
      "publish": true,
      "interval": 15,
      "pager_team": "plex",
      "occurrences": 3
    }
  }
}
